{
    "name": "rezzza/locale-voter-bundle",
    "description": "Fetch locale of project through voters",
    "keywords": ["locale", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rezzza/LocaleVoterBundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/LocaleVoterBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "2.1.*"
    },
    "autoload":     {
        "psr-0": { "Rezzza\\LocaleVoterBundle": "" }
    },
    "target-dir": "Rezzza/LocaleVoterBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
